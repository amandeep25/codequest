<script>

function myfunc()
{
  var a=document.myform.q1.value;

  if(a=="e")
  {
     alert("You got it right " );
      window.location.replace(" https://en.wikipedia.org/wiki/Avengers_(comics) ");
  }
  else
  {
    alert("Enter the correct answer" );
  }
}


setInterval("myFunction()", 1);

function myFunction() {
  var x = document.getElementById("demo");
if (document.hasFocus()) 
{ x.innerHTML = "The document has focus.";
 }
   
else 
{  x.innerHTML = "The document DOES NOT have focus.";
  alert(" Treasure hunt ends here! " );
  self.close();
 }
}
